                <TextBlock HorizontalAlignment="Left" Margin="8,20,0,0" TextWrapping="Wrap" Text="Associate" VerticalAlignment="Top" Width="73" Foreground="#FFCBC9C9" FontSize="10"/>
                <TextBlock HorizontalAlignment="Left" Margin="174,20,0,0" TextWrapping="Wrap" Text="Search" VerticalAlignment="Top" Width="73" Foreground="#FFCBC9C9" FontSize="10"/>
