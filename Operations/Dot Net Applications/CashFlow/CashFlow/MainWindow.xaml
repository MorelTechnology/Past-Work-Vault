                <ComboBox x:Name="cbAssociate" HorizontalAlignment="Left" Margin="8,35,0,0" VerticalAlignment="Top" Width="158" FontSize="16" DisplayMemberPath="AdjustedName" SelectedValuePath="ActiveDirectoryID" SelectionChanged="cbAssociate_SelectionChanged" >
                </ComboBox>
                <TextBox x:Name="ebFilter" HorizontalAlignment="Left" Height="27" Margin="172,35,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="96" TextChanged="ebFilter_TextChanged" KeyUp="ebFilter_KeyUp" FontWeight="Bold" Padding="0,3,0,4" />
