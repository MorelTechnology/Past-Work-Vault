                    <Button.Background>
                        <LinearGradientBrush EndPoint="0,1" StartPoint="0,0">
                            <GradientStop Color="#FFD49595" Offset="0"/>
                            <GradientStop Color="#FF973C3C" Offset="0.5"/>
                            <GradientStop Color="#FF974040" Offset="0.534"/>
                            <GradientStop Color="#FF6A0D0D" Offset="0.989"/>
                        </LinearGradientBrush>
                    </Button.Background>
                </Button>
                <TextBlock HorizontalAlignment="Left" Margin="303,20,0,0" TextWrapping="Wrap" Text="Special Tracking Group" VerticalAlignment="Top" Width="120" Foreground="#FFCBC9C9" FontSize="10"/>
                <ComboBox x:Name="cbSpecialTrackingGroup" HorizontalAlignment="Left" Margin="303,35,0,0" VerticalAlignment="Top" Width="120" FontSize="12" DisplayMemberPath="SpecialTrackingGroup" SelectedValuePath="SpecialTrackingGroup" SelectionChanged="cbSpecialTrackingGroup_SelectionChanged" Height="26"/>
            </Grid>

