                      dataType="System.String"
                      description="The SQL command to be executed."
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT
   -- ROW_NUMBER() OVER ( ORDER BY ( SELECT 1 ) ) AS cr_RIITMHDINT_ID ,
    'CPI' AS MIG_TYPE ,
    [Sapiens Owner Company] AS OWNER_COMPANY ,
    RI_Header_Seq_No AS HEADER_SEQ_NO ,
    'TBP' AS HEADER_STATUS ,
    [Sapiens Party ID] AS CAPARTY_ID ,
    'USD' AS CAITEM_CURRENCY ,
