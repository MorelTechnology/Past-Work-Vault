    '' AS CAOPENED_BY ,
    GETDATE() AS CAOPENED_DATE ,
    1 AS SUBSYSTEM_ID ,
    '' AS SUB_CLAIM_ID ,
    0 AS CONTRACT_UW_YEAR ,
    '' AS TYPE_AOB_ITEM ,
    0 AS POOL_ID ,
    0 AS CAREINSURER_ID ,
   CAST((SELECT MAX(RI_Header_Seq_No) FROM etl.RI_CashReceiptStaging_Data) AS CHAR(20))    AS [BATCH_ID] ,
    0 AS TARG_ACCOUNT_ID ,
    '' AS TARG_ACCOUNT_REF ,
    '' AS ON_BEHALF_REF ,
    '' AS TARGET_TEXT ,
    '' AS CONTRACT_TYPE ,
    D.Sys_Add_Dt AS Sys_Create_Dt ,
    D.Sys_Update_Dt AS Sys_Update_Dt
FROM
    etl.RI_CashReceiptStaging_Data D
WHERE
    D.Process_Status_Ind = 'P'
AND D.RI_Header_Seq_No &gt; 0</property>
