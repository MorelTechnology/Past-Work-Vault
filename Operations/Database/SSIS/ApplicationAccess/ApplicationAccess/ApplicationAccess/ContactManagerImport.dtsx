                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">select distinct (c.FirstName + ' ' + c.LastName) as AdjustedName,
		g.name as [group]
	,r.Name as RoleName
from ab_group g
join ab_groupuser gu
on gu.GroupID = g.ID
join ab_userrole ur
on gu.UserID = ur.UserID
join ab_role r
on ur.RoleID = r.ID
join ab_user u
on ur.UserID = u.id
join ab_contact c
on c.id = u.ContactID
where (c.FirstName + ' ' + c.LastName) not in ('CCAuthUser CC','Super User')</property>
