>0.5in</BottomMargin><Style /></Page><AutoRefresh>0</AutoRefresh><DataSources><DataSource Name="DBA_Rep"><DataSourceReference>DBA_Rep</DataSourceReference><rd:SecurityType xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">None</rd:SecurityType><rd:DataSourceID xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">09114578-d92e-45fa-9fc7-57d9b6141522</rd:DataSourceID></DataSource></DataSources><DataSets><DataSet Name="DatabaseServer"><Query><DataSourceName>DBA_Rep</DataSourceName><CommandText>SELECT
	DatabaseName,
	MAX (CASE Env WHEN 'Dev' THEN ServerList END) AS [Dev SERVER],
	MAX (CASE Env WHEN 'Test' THEN ServerList END) AS [Test SERVER],
	MAX (CASE Env WHEN 'Prod' THEN ServerList END) AS [Prod SERVER]
FROM
	(
	SELECT
		db.DatabaseName,
		slo.Location AS Env,
		
		STUFF(
		(SELECT CAST(',' + d2.[Server] AS VARCHAR(MAX))
		FROM dbo.[Databases] d2
		INNER JOIN
			dbo.ServerList_SSIS sli2
			ON d2.[Server] = sli2.[Server]
		WHERE d2.DatabaseName = db.DatabaseName
		AND sli2.LocationID = sli.LocationID
		FOR XML PATH('')
		), 1,1,'') AS ServerList
			
	FROM
		dbo.[Databases] db
	INNER JOIN
		dbo.ServerList_SSIS sli
		ON db.[Server] = sli.[Server]
	INNER JOIN
		dbo.Server_Location slo
		ON sli.LocationID = slo.LocationID

	WHERE
		db.DatabaseName NOT IN ('master', 'model', 'msdb', 'tempdb')
	AND db.DatabaseName NOT LIKE ('ReportServer%')
	) sq1
GROUP BY DatabaseName
ORDER BY DatabaseName</CommandText><rd:UseGenericDesigner xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">true</rd:UseGenericDesigner></Query><Fields><Field Name="DatabaseName"><DataField>DatabaseName</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.String</rd:TypeName></Field><Field Name="Dev_SERVER"><DataField>Dev SERVER</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.String</rd:TypeName></Field><Field Name="Test_SERVER"><DataField>Test SERVER</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.String</rd:TypeName></Field><Field Name="Prod_SERVER"><DataField>Prod SERVER</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.String</rd:TypeName></Field></Fields><Filters><Filter><FilterExpression>=Fields!DatabaseName.Value</FilterExpression><Operator>In</Operator><FilterValues><FilterValue>=Parameters!DatabaseName.Value</FilterValue></FilterValues></Filter></Filters></DataSet><DataSet Name="Database_Get"><Query><DataSourceName>DBA_Rep</DataSourceName><CommandText>SELECT DISTINCT db.DatabaseName
FROM            Databases AS db INNER JOIN
                         ServerList_SSIS AS sli ON db.Server = sli.Server
WHERE db.DatabaseName NOT IN ('master', 'model', 'msdb', 'tempdb')
ORDER BY db.DatabaseName</CommandText></Query><Fields><Field Name="DatabaseName"><DataField>DatabaseName</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.String</rd:TypeName></Field></Fields></DataSet></DataSets><ReportParameters><ReportParameter Name="DatabaseName"><DataType>String</DataType><DefaultValue><DataSetReference><DataSetName>Database_Get</DataSetName><ValueField>DatabaseName</ValueField></DataSetReference></DefaultValue><Prompt>Database</Prompt><ValidValues><DataSetReference><DataSetName>Database_Get</DataSetName><ValueField>DatabaseName</ValueField><LabelField>DatabaseName</LabelField></DataSetReference></ValidValues><MultiValue>true</MultiValue></ReportParameter></ReportParameters><Language>en-US</Language><ConsumeContainerWhitespace>true</ConsumeContainerWhitespace><rd:ReportUnitType xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">Inch</rd:ReportUnitType><rd:ReportID xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">4da60a60-ed29-4719-bcea-0147bdbc3a51</rd:ReportID></Report>