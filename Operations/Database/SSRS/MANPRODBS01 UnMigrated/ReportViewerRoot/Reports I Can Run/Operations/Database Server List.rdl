/RightMargin><TopMargin>0.5in</TopMargin><BottomMargin>0.5in</BottomMargin><Style /></Page><AutoRefresh>0</AutoRefresh><DataSources><DataSource Name="DBA_Rep"><DataSourceReference>DBA_Rep</DataSourceReference><rd:SecurityType xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">None</rd:SecurityType><rd:DataSourceID xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">a5a0cfbb-f659-4a4d-9279-2161c65323b7</rd:DataSourceID></DataSource></DataSources><DataSets><DataSet Name="ServerList"><Query><DataSourceName>DBA_Rep</DataSourceName><QueryParameters><QueryParameter Name="@Env"><Value>=Parameters!Env.Value</Value></QueryParameter></QueryParameters><CommandText>SELECT        ServerList_SSIS.Server, Server_Location.Location AS Environment, CASE (LEFT(SQL_Servers.ProductVersion, 4)) 
                         WHEN '10.5' THEN '2008 R2' WHEN '10.0' THEN '2008' WHEN '9.00' THEN '2005' WHEN '8.00' THEN '2000' ELSE 'No Info' END AS Ver, 
                         ServerList_SSIS.Purpose
FROM            ServerList_SSIS LEFT OUTER JOIN
                         SQL_Servers ON ServerList_SSIS.Server = SQL_Servers.Server INNER JOIN
                         Server_Location ON ServerList_SSIS.LocationID = Server_Location.LocationID
WHERE Server_Location.LocationID IN (@Env)
ORDER BY Environment, ServerList_SSIS.Server</CommandText><rd:UseGenericDesigner xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">true</rd:UseGenericDesigner></Query><Fields><Field Name="Server"><DataField>Server</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.String</rd:TypeName></Field><Field Name="Environment"><DataField>Environment</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.String</rd:TypeName></Field><Field Name="Ver"><DataField>Ver</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.String</rd:TypeName></Field><Field Name="Purpose"><DataField>Purpose</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.String</rd:TypeName></Field></Fields></DataSet><DataSet Name="Environments"><Query><DataSourceName>DBA_Rep</DataSourceName><CommandText>SELECT        Location, LocationID
FROM            Server_Location AS slo
ORDER BY Location</CommandText></Query><Fields><Field Name="Location"><DataField>Location</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.String</rd:TypeName></Field><Field Name="LocationID"><DataField>LocationID</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.Int16</rd:TypeName></Field></Fields></DataSet></DataSets><ReportParameters><ReportParameter Name="Env"><DataType>Integer</DataType><DefaultValue><DataSetReference><DataSetName>Environments</DataSetName><ValueField>LocationID</ValueField></DataSetReference></DefaultValue><Prompt>Environment</Prompt><ValidValues><DataSetReference><DataSetName>Environments</DataSetName><ValueField>LocationID</ValueField><LabelField>Location</LabelField></DataSetReference></ValidValues><MultiValue>true</MultiValue></ReportParameter></ReportParameters><Language>en-US</Language><ConsumeContainerWhitespace>true</ConsumeContainerWhitespace><rd:ReportUnitType xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">Inch</rd:ReportUnitType><rd:ReportID xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">1618d83d-d314-4c89-a158-790dd7694ba8</rd:ReportID></Report>