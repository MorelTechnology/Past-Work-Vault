  <DataSets>
    <DataSet Name="AggregateClaimsList">
      <Fields>
        <Field Name="ClaimNos">
          <DataField>ClaimNos</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>Reinsurance</DataSourceName>
        <CommandText>Declare @AggregateClaimList as varchar(MAX)

IF @OTWC_WC = 'OTWC'
	BEGIN
	
		SELECT @AggregateClaimList = ISNULL(@AggregateClaimList + ', ', '') + ClaimNo
		FROM dbo.AggregateClaim
		WHERE GroupID = (SELECT DISTINCT GroupID FROM dbo.AggregateClaim WHERE ClaimNo = @ClaimNo)

	END
ELSE IF @OTWC_WC = 'WC'
	BEGIN 

		SELECT @AggregateClaimList = ISNULL(@AggregateClaimList + ', ', '') + ClientClaimNo
		FROM dbo.AggregateClaim
		WHERE GroupID = (SELECT DISTINCT GroupID FROM dbo.AggregateClaim WHERE ClientClaimNo = @ClientClaimNo)

	END
	
SELECT @AggregateClaimList AS ClaimNos</CommandText>
        <QueryParameters>
          <QueryParameter Name="@OTWC_WC">
            <Value>=Parameters!OTWC_WC.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@ClaimNo">
            <Value>=Parameters!ClaimNo.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@ClientClaimNo">
            <Value>=Parameters!ClientClaimNo.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
  </DataSets>
