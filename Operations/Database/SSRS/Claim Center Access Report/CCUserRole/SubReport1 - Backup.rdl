  <DataSets>
    <DataSet Name="DataSet1">
      <Fields>
        <Field Name="Role">
          <DataField>Role</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Role_Description">
          <DataField>Role Description</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Permission_Name">
          <DataField>Permission Name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Permission_Description">
          <DataField>Permission Description</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>Prod_ClaimCenter</DataSourceName>
        <CommandType>Text</CommandType>
        <CommandText>SELECT 
	   R.Name as [Role]
	  ,R.[Description] as [Role Description]
	  ,PT.NAME as [Permission Name]
	  ,PT.[DESCRIPTION] as [Permission Description]
  FROM [dbo].[cc_privilege] P
  LEFT JOIN cctl_systempermissiontype PT ON P.Permission = PT.ID
  LEFT JOIN cc_role R ON R.ID = P.RoleID

  order by PT.name asc ,P.RoleID asc, P.ID</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <Timeout>0</Timeout>
      </Query>
    </DataSet>
  </DataSets>
