  <DataSets>
    <DataSet Name="GLEntryDuringPeriod_IFRS">
      <Query>
        <DataSourceName>sdsNavision</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@DateValidation">
            <Value>=Parameters!DateValidation.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@LegalEntity">
            <Value>=Parameters!SpecLegalEntity.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
          <QueryParameter Name="@ExcludeZero">
            <Value>=Parameters!ExcludeZero.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
          <QueryParameter Name="@EndDate">
            <Value>=Parameters!EndDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@StartDate">
            <Value>=Parameters!StartDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>IF @DateValidation = 1 BEGIN
SELECT EB.GL as 'G/L Account No.',GLA.Name as 'G/L Account Name',
       EB.SC as 'Subcos Code',EB.B as 'Basis Code',
       CONVERT(money,ISNULL(EndingBalance,0)-ISNULL(NetChange,0)) as 'Beginning Balance', CONVERT(money,ISNULL(NetChange,0)) as 'Net Change',CONVERT(money,ISNULL(EndingBalance,0)) as 'Ending Balance'
FROM

(SELECT GLE.[G_L Account No_] GL, GLE.[Global Dimension 1 Code] SC, GLE.[Global Dimension 2 Code] B,SUM(GLE.[Debit Amount])-SUM(GLE.[Credit Amount]) EndingBalance
FROM   [THC_RiverStone$G_L Entry] AS GLE
WHERE GLE.[Global Dimension 1 Code] IN (SELECT val FROM [dbo].[split]((SELECT [Subcos List] FROM [THC_RiverStone$Legal Entity] WHERE Code = @LegalEntity),',')) AND
GLE.[Global Dimension 2 Code] IN (SELECT val FROM [dbo].[split]((SELECT [IFRS Basis List] FROM [THC_RiverStone$Legal Entity] WHERE Code = @LegalEntity),',')) 
AND
GLE.[Posting Date] BETWEEN '17530101' AND @EndDate
GROUP BY GLE.[G_L Account No_],GLE.[Global Dimension 1 Code], GLE.[Global Dimension 2 Code]) AS EB

LEFT JOIN

(SELECT GLE.[G_L Account No_] GL, GLE.[Global Dimension 1 Code] SC, GLE.[Global Dimension 2 Code] B,SUM(GLE.[Debit Amount])-SUM(GLE.[Credit Amount]) NetChange
FROM   [THC_RiverStone$G_L Entry] AS GLE
WHERE GLE.[Global Dimension 1 Code] IN (SELECT val FROM [dbo].[split]       ((SELECT [Subcos List] FROM [THC_RiverStone$Legal Entity] WHERE Code = @LegalEntity),','))
AND
GLE.[Global Dimension 2 Code] IN (SELECT val FROM [dbo].[split]((SELECT [IFRS Basis List] FROM [THC_RiverStone$Legal Entity] WHERE Code = @LegalEntity),',')) 
AND
GLE.[Posting Date] BETWEEN @StartDate AND @EndDate
GROUP BY GLE.[G_L Account No_],GLE.[Global Dimension 1 Code], GLE.[Global Dimension 2 Code]) AS NC

ON NC.GL= EB.GL AND
   NC.SC = EB.SC AND
   NC.B = EB.B

LEFT JOIN [G_L Account] AS GLA 
ON GLA.No_ = EB.GL

WHERE
(@ExcludeZero = 0) OR
((ISNULL(EndingBalance,0) - ISNULL(NetChange,0)) &lt;&gt; 0) OR
(ISNULL(NetChange,0) &lt;&gt; 0) OR
(ISNULL(EndingBalance,0) &lt;&gt; 0)

ORDER BY EB.GL,EB.SC,EB.B
END</CommandText>
      </Query>
      <Fields>
        <Field Name="G_L_Account_No_">
          <DataField>G/L Account No.</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="G_L_Account_Name">
          <DataField>G/L Account Name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Subcos_Code">
